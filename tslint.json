{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "semicolon": [
      false,
      "never"
    ],
    "member-access": [
      false
    ],
    "no-var-requires": true,
    "quotemark": [
      true,
      "single",
      "jsx-single"
    ],
    "jsx-no-lambda": false,
    "no-console": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}